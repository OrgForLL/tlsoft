【2016-5-14 22:22:49】  declare @dhbh varchar(6);
                        select @dhbh=MAX(dhbh) from yx_t_dhryxx
                        select a.Cname,a.id as ryid,ISNULL(sjdept.wxid,0) as sjwxbm,ISNULL(dept.wxid,0) as wxbm,
                        sjkh.khid sjkhbm,sjkh.khmc as sjbmmc,kh.khid as khbm,kh.khmc as bmmc,ISNULL(wx.ID,0) wxid,ISNULL(wx.department,0) wxdept,a.rygx as position,case when a.Sex=1 then 1 else 2 end as gender
                        from yx_t_dhryxx a 
                        inner join yx_t_khb b on a.khid=b.khid
                        inner join yx_t_khb sjkh on dbo.split(b.ccid,'-',2)=sjkh.khid
                        inner join t_mdb c on a.mdid=c.mdid
                        inner join yx_t_khb kh on c.khid=kh.khid
                        left join wx_t_Deptment sjdept on sjkh.khid=sjdept.id and sjdept.deptType='my'
                        left join wx_t_Deptment dept on kh.khid=dept.id and dept.deptType='my'
                        left join wx_t_customers wx on wx.name=@name
                        where a.dhbh=@dhbh and a.Cname=@cname and a.PhoneNumber=@phoneNum and a.IdCard=@IdCard name:liqf cname:李清峰 phoneNum:15260825007 idcard:3505821989092828010
